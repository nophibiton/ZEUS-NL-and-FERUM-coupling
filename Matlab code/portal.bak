# ----------------------------------------------------------------------------------------------------
#                                             Analysis Type
# ----------------------------------------------------------------------------------------------------

analysis 3d static

# ----------------------------------------------------------------------------------------------------
#                                            Material Types
# ----------------------------------------------------------------------------------------------------

materials
 mat.name     model     properties
 rein         stl1      200000.   500.   0.005
 conf         con2      20.   2.2   0.002   1.2
 unc          con2      20.   2.2   0.002   1.02
 steel        stl0      200000.

# ----------------------------------------------------------------------------------------------------
#                                            Cross Sections
# ----------------------------------------------------------------------------------------------------

sections
 sec.name     type     mat.name              dimensions
 scol         rcrs     rein   unc   conf     400.   350.   300.   250. &
                                             255.    175.   125.    &
                                             127.5   175.   0.      &
                                             127.5   0.     125.   
 sbeam        rcts     rein   unc   conf     200.   600.   150.   600.   1250.   300.   1200.   250. &
                                             255.    25.    125.    &
                                             255.    775.   125.    &
                                             127.5   25.    0.      &
                                             127.5   775.   0.     
 stl_mem      rss      steel                 1000.   1000.

# ----------------------------------------------------------------------------------------------------
#                                            Element Classes
# ----------------------------------------------------------------------------------------------------

element.classes
 type.of.element = cubic
  grp.name     sec.name     monitoring.points
  col          stl_mem      200
  beam         stl_mem      200

# ----------------------------------------------------------------------------------------------------
#                                           Structural Nodes
# ----------------------------------------------------------------------------------------------------

structural.nodal.coordinates
 nod.name     x       y       z
 n111         0.      0.      0.
 n121         0.      3000.   0.
 n211         4000.   0.      0.
 n221         4000.   3000.   0.

# ----------------------------------------------------------------------------------------------------
#                                         Non-Structural Nodes
# ----------------------------------------------------------------------------------------------------

non.structural.nodal.coordinates
 nod.name     x       y       z
 nsn1001      8000.   6000.   0.

# ----------------------------------------------------------------------------------------------------
#                                         Element Connectivity
# ----------------------------------------------------------------------------------------------------

element.connectivity
  elm.name     grp.name     nod.name(s)
  col111       col          n111   n121   nsn1001
  bmx121       beam         n121   n221   nsn1001
  col211       col          n211   n221   nsn1001

# ----------------------------------------------------------------------------------------------------
#                                              Restraints
# ----------------------------------------------------------------------------------------------------

restraints
  nod.name     direction
  n111         x+y+z+rx+ry+rz
  n121         z+rx+ry
  n211         x+y+z+rx+ry+rz
  n221         z+rx+ry

# ----------------------------------------------------------------------------------------------------
#                                            Applied Loading
# ----------------------------------------------------------------------------------------------------

applied.loading

 initial.loads
   nod.name     direction     type     value
   n121         y             force    -1000000.
   n221         y             force    -5000000.

# ----------------------------------------------------------------------------------------------------
#                                          Iterative Strategy
# ----------------------------------------------------------------------------------------------------

iterative.strategy
  number.of.iterations = 20
  initial.reformations = 20
  step.reduction       = 5
  divergence.iteration = 15
  maximum.convergence = 1000.

# ----------------------------------------------------------------------------------------------------
#                                         Convergence Criteria
# ----------------------------------------------------------------------------------------------------

convergence.criteria
  tolerance = 0.0001
   displacement.ref = 1000.
   rotation.ref     = 1.

# ----------------------------------------------------------------------------------------------------
#                                                Output
# ----------------------------------------------------------------------------------------------------

output
  frequency 0

# ----------------------------------------------------------------------------------------------------
#                                              End of File
# ----------------------------------------------------------------------------------------------------

end
